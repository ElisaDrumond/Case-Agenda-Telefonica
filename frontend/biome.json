{
	"$schema": "https://biomejs.dev/schemas/1.8.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}, "javascript": {
		"formatter": {
			"enabled": true,
			"indentStyle": "space",
			"trailingCommas": "none"
		}
	}, "json": {
		"formatter": {
			"enabled": false
		}
	}
}
